    "test:instagram:v2:comprehensive": "ts-node tests/instagram-v2-comprehensive.ts",
    "test:instagram:v2:reliability": "ts-node tests/instagram-v2-reliability.ts",
    "test:instagram:v2:affiliate-accuracy": "ts-node tests/instagram-v2-affiliate-accuracy.ts",
    "test:instagram:v2:comparison": "ts-node tests/instagram-v1-vs-v2-comparison.ts",
    "test:instagram:v2:dashboard": "ts-node tests/instagram-v2-dashboard.ts",
    "test:instagram:v2:dashboard:quick": "ts-node tests/instagram-v2-dashboard.ts -- --quick",
    "test:instagram:v2:dashboard:monitor": "ts-node tests/instagram-v2-dashboard.ts -- --monitor",
    "test:instagram:v2:all": "npm run test:instagram:v2:comprehensive && npm run test:instagram:v2:reliability && npm run test:instagram:v2:affiliate-accuracy && npm run test:instagram:v2:comparison",
    "monitor": "ts-node tests/monitoring/monitor.ts"
