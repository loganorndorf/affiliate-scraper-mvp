    "test:platform": "ts-node tests/run.ts -- --platform",
    "test:fixed": "ts-node tests/test-fixed-extractors.ts",
    "test:all": "ts-node tests/test-all-platforms.ts",
    "test:all:quick": "ts-node tests/test-all-platforms.ts -- --mode=quick",
    "test:instagram:mobile": "ts-node tests/test-instagram-mobile-api.ts",
